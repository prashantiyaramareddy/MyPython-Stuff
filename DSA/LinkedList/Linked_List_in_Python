        "### Simple Linked List implementation in Python"
      ],
      "metadata": {
        "id": "YAl8RCkR92oU"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "class Node:\n",
        "    def __init__(self, k):\n",
        "        self.key = k\n",
        "        self.next = None\n",
        "\n",
        "\n",
        "temp1 = Node(10)\n",
        "temp2 = Node(20)\n",
        "temp3 = Node(30)\n",
        "\n",
        "temp1.next = temp2\n",
        "temp2.next = temp3\n",
        "\n",
        "head = temp1\n",
        "print(head.key)\n",
        "print(head.next.key)\n",
        "print(head.next.next.key)"
